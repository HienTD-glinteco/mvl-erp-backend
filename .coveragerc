[run]
source = apps,lib
concurrency = multiprocessing
parallel = true
omit =
    static*
    logs*
    *migrations*
    tests*
    */__init__.py
    */conftest.py
    */test_*.py
    */tests/*

[report]
exclude_lines =
    pragma: no cover
    def __unicode__
    def __repr__
    if __name__ == .__main__.:

[html]
title = Coverage Report
