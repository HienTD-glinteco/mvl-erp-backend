# Password validators
msgid "Password must contain at least one uppercase letter."
msgstr "Mật khẩu phải chứa ít nhất một chữ cái viết hoa."

msgid "Password must contain at least one lowercase letter."
msgstr "Mật khẩu phải chứa ít nhất một chữ cái viết thường."

msgid "Password must contain at least one digit."
msgstr "Mật khẩu phải chứa ít nhất một chữ số."

msgid "Password must contain at least one special character (!@#$%^&*(),.?\":{}|<>)."
msgstr "Mật khẩu phải chứa ít nhất một ký tự đặc biệt (!@#$%^&*(),.?\":{}|<>)."

msgid "Password must be at least 8 characters long and include uppercase, lowercase, digits and special characters."
msgstr "Mật khẩu phải có tối thiểu 8 ký tự, bao gồm chữ hoa, chữ thường, số và ký tự đặc biệt."

# Login serializer
msgid "Password"
msgstr "Mật khẩu"

msgid "Please enter your username."
msgstr "Vui lòng nhập tên đăng nhập."

msgid "Username cannot be blank."
msgstr "Tên đăng nhập không được để trống."

msgid "Please enter your password."
msgstr "Vui lòng nhập mật khẩu."

msgid "Password cannot be blank."
msgstr "Mật khẩu không được để trống."

msgid "Username does not exist."
msgstr "Tên đăng nhập không tồn tại."

msgid "Account is locked. Please try again in %(minutes)d minutes."
msgstr "Tài khoản đã bị khóa. Vui lòng thử lại sau %(minutes)d phút."

msgid "Account has been deactivated."
msgstr "Tài khoản đã bị vô hiệu hóa."

msgid "Account has been locked due to 5 failed login attempts. Please try again in 5 minutes."
msgstr "Tài khoản đã bị khóa do đăng nhập sai quá 5 lần. Vui lòng thử lại sau 5 phút."

msgid "Incorrect password."
msgstr "Mật khẩu không đúng."

# Password change serializer
msgid "Current password"
msgstr "Mật khẩu hiện tại"

msgid "Please enter your current password."
msgstr "Vui lòng nhập mật khẩu hiện tại."

msgid "Current password cannot be blank."
msgstr "Mật khẩu hiện tại không được để trống."

msgid "New password"
msgstr "Mật khẩu mới"

msgid "Please enter your new password."
msgstr "Vui lòng nhập mật khẩu mới."

msgid "New password cannot be blank."
msgstr "Mật khẩu mới không được để trống."

msgid "Confirm new password"
msgstr "Xác nhận mật khẩu mới"

msgid "Please confirm your new password."
msgstr "Vui lòng xác nhận mật khẩu mới."

msgid "Password confirmation cannot be blank."
msgstr "Xác nhận mật khẩu không được để trống."

msgid "New password and confirmation password do not match."
msgstr "Mật khẩu mới và xác nhận mật khẩu không khớp."

msgid "New password must be different from current password."
msgstr "Mật khẩu mới phải khác với mật khẩu hiện tại."

msgid "You need to be logged in to change your password."
msgstr "Bạn cần đăng nhập để thay đổi mật khẩu."

msgid "Current password is incorrect."
msgstr "Mật khẩu hiện tại không đúng."

# Password reset
