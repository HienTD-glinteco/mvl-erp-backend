msgstr "Không tìm thấy tài khoản với thông tin này."

# OTP verification
msgid "OTP code can only contain digits."
msgstr "Mã OTP chỉ được chứa số."

msgid "OTP code is incorrect or has expired."
msgstr "Mã OTP không đúng hoặc đã hết hạn."

msgid "OTP code must be 6 digits."
msgstr "Mã OTP phải có 6 chữ số."

msgid "Please enter the OTP code."
msgstr "Vui lòng nhập mã OTP."

msgid "Device ID of client app (browser can skip)"
msgstr "Device ID của client app (browser bỏ qua)"

# Views
msgid "Login with username and password"
msgstr "Đăng nhập với tên đăng nhập và mật khẩu"

msgid "OTP sent successfully"
msgstr "OTP đã được gửi thành công"

